#!/usr/bin/env ruby

require 'rubygems' unless defined?(Gem)
require 'beaker-hiera'

VERSION_STRING =
  "
                                 _ .--.
                                ( `    )
        beaker-hiera         .-'      `--,
                  _..----.. (             )`-.
                .'_|` _|` _|(  .__,           )
               /_|  _|  _|  _(        (_,  .-'
              ;|  _|  _|  _|  '-'__,--'`--'
              | _|  _|  _|  _| |
          _   ||  _|  _|  _|  _| %s
        _( `--.\\_|  _|  _|  _|/
     .-'       )--,|  _|  _|.`
    (__, (_      ) )_|  _| /
      `-.__.\\ _,--'\\|__|__/
                    ;____;
                     \\YT/
                      ||
                     |\"\"|
                     '=='
".freeze

puts format(VERSION_STRING, BeakerHiera::Version::STRING)

exit 0
